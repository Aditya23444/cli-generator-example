{
  "app": {
    "src": "src",
    "packager": "yarn@>= 0.18.0",
    "providers": [
      "primus"
    ]
  },
  "services": {
    "user": {
      "name": "user",
      "adapter": "memory",
      "path": "/user",
      "requiresAuth": false,
      "graphql": true
    },
    "comment": {
      "name": "comment",
      "adapter": "memory",
      "path": "/comment",
      "requiresAuth": false,
      "graphql": true
    },
    "like": {
      "name": "like",
      "adapter": "memory",
      "path": "/like",
      "requiresAuth": false,
      "graphql": true
    },
    "post": {
      "name": "post",
      "adapter": "memory",
      "path": "/post",
      "requiresAuth": false,
      "graphql": true
    },
    "relationship": {
      "name": "relationship",
      "adapter": "memory",
      "path": "/relationship",
      "requiresAuth": false,
      "graphql": true
    }
  },
  "graphql": {
    "path": "graphql",
    "resolvers": [
      "resolvers"
    ]
  }
}