{
  "name": "@feathers-x/cli-generator-example",
  "description": "An example generated by the Feathers-Plus cli focused on GraphQL.",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "John Szwaronek",
    "email": "johnsz9999@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "run": "npm link @feathers-plus/graphql; reset; node ./src/index.js",
    "help": "node ./node_modules/@feathers-x/cli/lib/index",
    "app": "node ./node_modules/@feathers-x/cli/lib/index generate app",
    "service": "npm link @feathers-x/cli; reset; node ./node_modules/@feathers-x/cli/lib/index generate service",
    "graph": "npm link @feathers-x/cli; reset; node ./node_modules/@feathers-x/cli/lib/index generate graphql",
    "swapi": "node ./poc/schema.test.js",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "start": "node src/",
    "mocha": "mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@feathers-plus/batch-loader": "0.3.0",
    "@feathersjs/configuration": "1.0.1",
    "@feathersjs/errors": "3.0.0",
    "@feathersjs/express": "1.1.2",
    "@feathersjs/feathers": "3.0.1",
    "@feathersjs/socketio": "3.0.1",
    "compression": "1.7.1",
    "cors": "2.8.4",
    "deep-equal": "1.0.1",
    "feathers-hooks-common": "git://github.com/feathers-plus/feathers-hooks-common.git#master",
    "feathers-memory": "1.3.1",
    "feathers-nedb": "2.7.0",
    "graphql": "0.11.7",
    "helmet": "3.9.0",
    "lodash": "4.17.4",
    "merge-graphql-schemas": "1.3.0",
    "nedb": "1.8.0",
    "serve-favicon": "2.4.5",
    "sqlite": "2.9.0",
    "sqlite3": "3.1.13",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "eslint": "4.11.0",
    "mocha": "4.0.1",
    "request": "2.83.0",
    "request-promise": "4.2.2"
  }
}
